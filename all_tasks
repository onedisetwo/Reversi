void task_switch(short task_number){ //Выбор заданий
    switch(task_number){
    case 1: //+
        task1();
        break;
    case 2: //+
        task2();
        break;
    case 3:
        task3();
        break;
    case 4:
        task4();
        break;
    default:
        cout<<"Not yet";
        break;
    }
}

