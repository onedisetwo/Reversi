#include <iostream>
#include <vector>
#include <string>
#define debug; cin>>debugx;

using namespace std;

vector<vector<short> > board, board2;
bool debugx;

short difplayer(short player){
    player++;
    if(player>2)
        player=1;
    return player;
}

void board_start(){ //Изменение размера и заполнение до стартовой позиции
    board.resize(9);
    for(int i=0;i<9;i++){
        board[i].resize(9);
    }
    for(int i=0;i<9;i++){
        for(int k=0;k<9;k++){
            board[i][k]=0;
            if(i==4 & k==4 || i==5 & k==5){
                board[i][k]=1;
            }
            if(i==5 & k==4 || i==4 & k==5){
                board[i][k]=2;
            }
        }
    }
}

void board_input(){ //Ввод доски
    for(int i=1;i<9;i++){
        string s;
        cin>>s;
        for(int k=1;k<9;k++){
            board[9-i][k]=(int)(s[k-1])-48;
        }
    }
}

void board2_input(){ //Ввод доски
    for(int i=1;i<9;i++){
        string s;
        cin>>s;
        for(int k=1;k<9;k++){
            board2[9-i][k]=(int)(s[k-1])-48;
        }
    }
}

void board_print(){ //Напечатать доску
    for(int i=1;i<9;i++){
        for(int k=1;k<9;k++){
            cout<<board[9-i][k];
        }
        cout<<endl;
    }
}

void move_make1(short x,short y,short player){ //Ход через два числа
    board[y][x]=player;
    //player=difplayer(player);
/*первая коорд. - буква, вторая цифра. т.е.
5 - - - - -
4 - - - - -
3 - - - - -
2 - - - - -
1 - - - - -
- A B C D E
*/
}

void move_make2(string s,short player){ //Ход через строку вида #A3
    move_make1((int)(s[0])-64,(int)(s[1])-48,player);
}

void move_undo(short x,short y){
    board[y][x]=0;
}

string numb_latin(short x, short y){ //Перевод из числовых координат в #A1
    string s="00";
    s[0]=(char)(x+64);
    s[1]=(char)(y+48);
    return s;
}

vector<bool> vec_move_possibility(short x, short y,short player){
    vector<bool> v(8,0);
    /*
    - - - - -
    - 2 3 4 -
    - 1 X 5 -
    - 0 7 6 -
    - - - - -
    */
    move_make1(y,x,player);
    if(v[0]==0 && board[y-1][x-1]==difplayer(player)){
        for(int i=2;y-i>0 && x-i>0;i++){
            if(board[y-i][x-i]==0){
                break;
            }
            if(board[y-i][x-i]==player){
                v[0]=1;
            }
        }
    }
    if(v[1]==0 && board[y-1][x]==difplayer(player)){
        for(int i=2;y-i>0;i++){
            if(board[y-i][x]==0){
                break;
            }
            if(board[y-i][x]==player){
                v[1]=1;
            }
        }
    }
    if(v[2]==0 && board[y-1][x+1]==difplayer(player)){
        for(int i=2;y-i>0 && x+i<8;i++){
            if(board[y-i][x+i]==0){
                break;
            }
            if(board[y-i][x+i]==player){
                v[2]=1;
            }
        }
    }
    if(v[3]==0 && board[y][x+1]==difplayer(player)){
        for(int i=2;x+i<8;i++){
            if(board[y][x+i]==0){
                break;
            }
            if(board[y][x+i]==player){
                v[3]=1;
            }
        }
    }
    if(v[4]==0 && board[y+1][x+1]==difplayer(player)){
        for(int i=2;y+i<8 && x+i<8;i++){
            if(board[y+i][x+i]==0){
                break;
            }
            if(board[y+i][x+i]==player){
                v[4]=1;
            }
        }
    }
    if(v[5]==0 && board[y+1][x]==difplayer(player)){
        for(int i=2;y+i<8;i++){
            if(board[y+i][x]==0){
                break;
            }
            if(board[y+i][x]==player){
                v[5]=1;
            }
        }
    }
    if(v[6]==0 && board[y+1][x-1]==difplayer(player)){
        for(int i=2;y+i<8 && x-i>0;i++){
            if(board[y+i][x-i]==0){
                break;
            }
            if(board[y+i][x-i]==player){
                v[6]=1;
            }
        }
    }
    if(v[7]==0 && board[y][x-1]==difplayer(player)){
        for(int i=2;x-i>0;i++){
            if(board[y][x-i]==0){
                break;
            }
            if(board[y][x-i]==player){
                v[7]=1;
            }
        }
    }
    return v;
}

bool bool_move_possibility(short x, short y,short player){
    vector<bool> v=vec_move_possibility(x,y,player);
    for(auto i:v){
        if(i==1)
            return 1;
    }
    return 0;
}

vector<vector<short> > move_variants(){ //Варианты хода из заданного положения
    for(int i=1;i<9;i++){
        for(int k=1;k<9;k++){
            
        }
    }
}

vector<short> wincheck_3(){ //Проверка выигрыша: победитель, кол-во черных, кол-во белых
    vector<short> v(3,0);
    for(int i=1;i<9;i++){
        for(int k=1;k<9;k++){
            v[board[i][k]]++;
        }
    }
    if(v[1]>v[2])
        v[0]=1;
    if(v[2]>v[1])
        v[0]=2;
    if(v[1]==v[2])
        v[0]=0;
    return v;
}

short wincheck_player(){ //Проверка выигрыщша: победитель
    return wincheck_3()[0];
}

